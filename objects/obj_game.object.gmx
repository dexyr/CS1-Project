        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Frame Stuff

global.counter++;

input_enter = keyboard_check_pressed(vk_enter);

if game_state == game_states.MENU
{
    focus_obj = instance_find(obj_player_spawn, 0);
    
    cam.intended_x = focus_obj.x;
    cam.intended_y = focus_obj.y;
    
    if input_enter
    {
        //Create Ghosts
        
        for(var i = 0; i &lt; instance_number(obj_ghost_spawn); i++)
        {
            var spawn_obj = instance_find(obj_ghost_spawn, i);
            instance_create(spawn_obj.x, spawn_obj.y, obj_ghost);
        }
        //Create Player
        
        var spawn_obj = instance_find(obj_player_spawn, 0);
        var player_obj = instance_create(spawn_obj.x, spawn_obj.y, obj_player);
        cam.player = player_obj;
        
        game_state = game_states.PLAY;
    }
}
if game_state == game_states.PLAY
{
    if instance_number(obj_ghost) == 0 or instance_number(obj_player) == 0
    {
        //Determine if the game is won or lost
        
        if instance_number(obj_ghost) == 0
        {
            game_state = game_states.WIN;
        }
        if instance_number(obj_player) == 0
        {
            game_state = game_states.LOSE;
        }
        
        //Remove the camera target
        
        cam.player = noone;
        
        //Deactivate the player
        
        if instance_number(obj_player) &gt; 0
        {
            var player_obj = instance_find(obj_player, 0);
            player_obj.active = false;
        }
    }
}
if game_state == game_states.LOSE or game_state == game_states.WIN
{
    if input_enter
    {
        game_state = game_states.MENU;
        
        //"Delete" all chars by setting their hp to 0
        
        for(var i = 0; i &lt; instance_number(obj_ghost); i++)
        {
            var ghost_obj = instance_find(obj_ghost, i);
            ghost_obj.hp = 0;
        }
        
        if instance_number(obj_player) &gt; 0
        {
            var player_obj = instance_find(obj_player, 0);
            player_obj.hp = 0;
        }
    }
}
