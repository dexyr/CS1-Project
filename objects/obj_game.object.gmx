            <string>///Draw Menus

if global.counter % 45 == 0
{
    blink = !blink;
}

draw_set_colour(c_white);
draw_set_halign(fa_center);
draw_set_valign(fa_middle);
var x_center = (view_wview / 2) + view_xview;
var y_center = (view_hview / 2) + view_yview;

if game_state == game_states.MENU
{
    draw_set_font(fnt_title);
    draw_text(x_center, y_center - 80, "A CS1 Project");
    draw_set_font(fnt_menu);
    draw_text(x_center, y_center + 80, "Sounds from bfxr.net");
    
    if blink
    {
        draw_set_font(fnt_menu);
        draw_text(x_center, y_center, "PRESS ENTER TO START");
    }
}
if game_state == game_states.WIN
{
    draw_set_font(fnt_menu);
    draw_text(x_center, y_center, "YOU WON!");
    
    if blink
    {
        draw_text(x_center, y_center + 50, "PRESS ENTER TO RESET");
    }
}
if game_state == game_states.LOSE
{
    draw_set_font(fnt_menu);
    draw_text(x_center, y_center, "YOU LOST!   D:");
    
    if blink
    {
        draw_text(x_center, y_center + 50, "PRESS ENTER TO RESET");
    }
}
